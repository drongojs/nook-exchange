{
  "name": "nook-exchange",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "snowpack",
    "lint": "eslint --ext \"ts,tsx\" src && tsc --noEmit",
    "build:modules": "snowpack --clean --optimize",
    "build:js": "yarn babel -d \"lib\" --no-comments --compact --minified --source-maps --delete-dir-on-start -x \".ts,.tsx\" \"src\"",
    "build:html": "ejs-cli -f index.ejs -o ./public -b src/presentation",
    "build:post": "cp -r lib/ public/ && cp -r web_modules/ public/",
    "build": "yarn build:modules && yarn build:js && yarn build:html && yarn build:post",
    "start:dev": "browser-refresh devServer"
  },
  "main": "index.js",
  "repository": "git@github.com:drongojs/nook-exchange.git",
  "author": "Jack Ellis <jack.ellis@godaddy.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-query": "^1.1.2",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-refresh": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "ejs": "^3.0.2",
    "ejs-cli": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "rollup-plugin-postcss": "^2.5.0",
    "snowpack": "^1.6.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "copy-to-clipboard": "^3.3.1",
    "jpex": "^3.1.0",
    "react": "npm:@pika/react",
    "react-dom": "npm:@pika/react-dom",
    "react-query": "^1.2.5",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "use-debounce": "^3.4.1"
  }
}
